<application>
  <component name="LanguageInjectionConfiguration">
    <injection language="DTD" injector-id="xml">
      <display-name>item/@state_pressed</display-name>
      <place><![CDATA[xmlAttribute().withLocalName(string().equalTo("state_pressed")).withNamespace(string().equalTo("http://schemas.android.com/apk/res/android")).withParent(xmlTag().withLocalName(string().equalTo("item")))]]></place>
    </injection>
    <injection language="DTD" injector-id="xml">
      <display-name>manifest</display-name>
      <place><![CDATA[xmlTag().withLocalName(string().equalTo("manifest"))]]></place>
    </injection>
  </component>
</application>